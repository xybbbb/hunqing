                        {
                            <a href="#" >
                                <img src="/Content/img/Lunbo1/@item.img" alt="加载中..." />
                            </a>
                        }
                        @*<a href="#" v-for="it in im">
            <img :src="lv+it.img" alt="加载中..." />
        </a>*@

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

    <!-- 饿了么UI -->
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css" />
    @Scripts.Render("~/bundles/modernizr")
    <style>
        .product {
            background-color: #fff;
            padding: 20px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            margin-bottom: 20px;
        }

        .product-body {
            background-color: #fff;
            border: 1px solid rgba(0, 0, 0, 0.1);
        }

        .product-title {
            font-size: 1.2rem;
            font-weight: bold;
            padding: 10px;
            margin: 0;
        }

        .product-description {
            font-size: 1rem;
            color: #666;
            margin-bottom: 10px;
        }
    </style>
</head>

